<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table class="table">
  <thead class="thead-dark">
  <tr>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Novo Usuario', class: 'btn btn-primary' %>